{
  "name": "go_composer/starter_theme",
  "description": "GO : Grow Online Starter theme to build any website",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "email": "jared@upstatement.com",
      "name": "jarednova"
    },
    {
      "email": "julien.growonline@gmail.com",
      "name": "Julien Scheen"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "require": {
    "phpmailer/phpmailer": "^6.5"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.16|6.*"
  }
}
